= form_for(@team, html: {id: "new-team-form"}) do |f|

  %div
    = f.label :title

  %div
    = f.text_field :title

  %div
    = f.label :age_group, 'Division'

  %div
    = f.select(:age_group, options_for_select(@divisions.collect{ |d| [d,{class: 'some-class'} ]} ))


  %div
    = f.label :num_of_players

  %div
    = f.text_field :num_of_players

  %div
    =f.submit
