%table.all-players
  #table-header All Players List
  %tr
    %th First Name
    %th Last Name
    %th Address
    %th Age
    %th Team
  - @all_players.each do |player|
    %tr
      %td
        = player.first_name
      %td
        = player.last_name
      %td
        = "#{player.address}, #{player.town}, #{player.zip}"
      %td
        = "#{player.age}"
      %td
        = team_player_belongs_to(player)



