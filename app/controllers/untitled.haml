- if @active_team == true
  .teamBrand.center
    = image_tag('uniondale knight.png', size: '300x300')
    .teamBanner
      %h3= "#{@active_team.title.upcase} Football Team"
      %h5= "#{@active_team.age_group}"
      %p
        Current number of players:
        = @active_team.num_of_players
- else
  = @active_team.title
  %h1= "No team currently active"